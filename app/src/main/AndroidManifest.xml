<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.qq.qzone.a133689237.fo">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".welcome"
            android:screenOrientation="portrait"
            android:theme="@style/welcomeTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".jishuActivity"
            android:screenOrientation="portrait"
            android:theme="@style/jishuTheme">
        </activity>
        <activity
            android:name=".luanhuanActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:parentActivityName=".SettingsActivity"
            android:name=".fankuiActivity"
            android:label="意见反馈"
            android:screenOrientation="portrait"
            android:theme="@style/fankuiTheme">
        </activity>
        <activity
            android:parentActivityName=".SettingsActivity"
            android:name=".guanyuActivity"
            android:label="关于作者"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".webActivity"
            android:label="佛心禅闻"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:parentActivityName=".SettingsActivity"
            android:name=".chongzhiActivity"
            android:label="天数设定"
            android:screenOrientation="portrait">
        </activity>

        <receiver android:name=".guangbo"
                  android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE">
                </action>
            </intent-filter>
        </receiver>

        <activity
            android:parentActivityName=".MainActivity"
            android:name=".SettingsActivity"
            android:screenOrientation="portrait"
            android:label="设置">
        </activity>
    </application>

</manifest>