<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qq.qzone.a133689237.fo">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".welcome"
            android:screenOrientation="portrait"
            android:theme="@style/welcomeTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/zhuStyle"
            android:screenOrientation="portrait" />
        <activity
            android:name=".jishuActivity"
            android:screenOrientation="portrait"
            android:theme="@style/jishuTheme" />
        <activity
            android:name=".fankuiActivity"
            android:label="@string/yijianfankui"
            android:parentActivityName=".SettingsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/fankuiTheme"
            />
        <activity
            android:name=".guanyuActivity"
            android:label="@string/guanyuzuozhe"
            android:parentActivityName=".SettingsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".webActivity"
            android:label="@string/foxinchanwen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".chongzhiActivity"
            android:label="@string/tianshushedin"
            android:parentActivityName=".SettingsActivity"
            android:screenOrientation="portrait" />

        <receiver
            android:name=".guangbo"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" > </action>
            </intent-filter>
        </receiver>

        <activity
            android:name=".SettingsActivity"
            android:label="@string/shezhi"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".tuijianqumu_Activity"
            android:label="@string/tuijianqumu"
            android:parentActivityName=".SettingsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".juanzengActivity"
            android:screenOrientation="portrait"
            android:theme="@style/juanStyle" >

        </activity>
    </application>

</manifest>